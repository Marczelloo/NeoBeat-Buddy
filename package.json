{
  "name": "neo-beat-buddy",
  "version": "0.0.19",
  "description": "A Discord music bot",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "start:dev": "dotenv -e .env.dev -- pnpm start",
    "start:prod": "dotenv -e .env.prod -- pnpm start",
    "docker:prod": "docker compose up -d --build",
    "deploy": "node deploy-commands.js",
    "deploy:dev": "dotenv -e .env.dev -- node deploy-commands.js",
    "deploy:prod": "dotenv -e .env.prod -- node deploy-commands.js",
    "lavalink:dev": "docker compose -f helpers/lavalink/docker-compose.yml up -d",
    "lavalink:dev:down": "docker compose -f helpers/lavalink/docker-compose.yml down",
    "lint": "eslint . --ext .js,.cjs,.mjs",
    "lint:fix": "eslint . --ext .js,.cjs,.mjs --fix"
  },

  "keywords": [],
  "author": "Marczelloo",
  "license": "ISC",
  "packageManager": "pnpm@10.15.0",
  "dependencies": {
    "@discordjs/voice": "^0.19.0",
    "discord.js": "^14.22.1",
    "dotenv": "^17.2.2",
    "poru": "^5.2.0",
    "shoukaku": "^4.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.36.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-unused-imports": "^4.2.0",
    "globals": "^16.4.0"
  }
}
