{
  "name": "neo-beat-buddy",
  "version": "1.0.3",
  "description": "Educational music bot example (NOT for production use) | A feature-rich Discord music bot with DJ mode, advanced statistics, and persistent EQ settings",
  "private": true,
  "license": "SEE LICENSE IN LICENSE",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "start:dev": "dotenv -e .env.dev -- pnpm start",
    "start:prod": "dotenv -e .env.prod -- pnpm start",
    "docker:prod": "docker compose up -d --build",
    "docker:build": "docker buildx build --platform linux/arm64 -t marczelloo/neo-beat-buddy:latest --push .",
    "deploy": "node deploy-commands.js",
    "deploy:dev": "dotenv -e .env.dev -- node deploy-commands.js",
    "deploy:prod": "dotenv -e .env.prod -- node deploy-commands.js",
    "lavalink:dev": "docker compose -f helpers/lavalink/docker-compose.yml --env-file .env.dev up -d",
    "lavalink:dev:down": "docker compose -f helpers/lavalink/docker-compose.yml down",
    "lint": "eslint . --ext .js,.cjs,.mjs",
    "lint:fix": "eslint . --ext .js,.cjs,.mjs --fix",
    "test": "node --test tests/**/*.test.js",
    "test:watch": "node --test --watch tests/**/*.test.js",
    "test:autoplay": "node --test tests/helpers/lavalink/smartAutoplay.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Marczelloo/NeoBeat-Buddy.git"
  },
  "keywords": [
    "discord",
    "music-bot",
    "lavalink",
    "discord-bot",
    "music",
    "discord.js",
    "poru",
    "equalizer",
    "dj-mode",
    "statistics"
  ],
  "author": "Marczelloo",
  "bugs": {
    "url": "https://github.com/Marczelloo/NeoBeat-Buddy/issues"
  },
  "homepage": "https://github.com/Marczelloo/NeoBeat-Buddy#readme",
  "packageManager": "pnpm@10.15.0",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=10.0.0"
  },
  "dependencies": {
    "@discordjs/voice": "^0.19.0",
    "cheerio": "^1.1.2",
    "discord.js": "^14.22.1",
    "dotenv": "^17.2.2",
    "node-fetch": "^2.7.0",
    "poru": "^5.2.0",
    "shoukaku": "^4.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.36.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-unused-imports": "^4.2.0",
    "globals": "^16.4.0"
  }
}
